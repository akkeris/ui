{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "testcafe"
  ],
  "rules": {
    "no-plusplus": "off",
    "no-underscore-dangle": "off"
  }
}